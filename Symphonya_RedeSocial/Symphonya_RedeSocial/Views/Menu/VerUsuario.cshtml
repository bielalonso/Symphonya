<!DOCTYPE HTML>
<html>
<head>
    <link rel="icon" href="~/Imagens/sympIcon.png" type="image/x-icon" /> 
    <title>Symphonya</title>
    <link rel="icon" href="~/Imagens/symphonyaIcon.png" type="image" />
    <link rel="shortcut icon" href="~/Imagens/symphonyaIcon.png" type="image" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <meta name="keywords" content="Augment Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Bootstrap Core CSS -->
    <link href="~/CSS/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom CSS -->
    <link href="~/CSS/style.css" rel='stylesheet' type='text/css' />
    <!-- Graph CSS -->
    <link href="~/CSS/font-awesome.css" rel="stylesheet">
    <!-- jQuery -->
    <link href='//fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css'>
    <!-- lined-icons -->
    <link rel="stylesheet" href="~/CSS/icon-font.min.css" type='text/css' />
    <!-- /js -->
    <script src="~/JS/jquery-1.10.2.min.js"></script>
    <!-- //js-->
</head>
<body>
    <div class="page-container">
        <!--/content-inner-->
        <div class="left-content">
            <div class="inner-content">
                <!-- header-starts -->
                <div class="header-section">
                    <!--menu-right-->
                    <div class="top_menu">
                        <div class="main-search">
                            <form method="post" action="~/Menu/Pesquisar">
                                <input id="busca" name="busca" type="text" value="Buscar" onFocus="this.value = '';" onBlur="if (this.value == '') {this.value = 'Buscar';}" class="text" />
                                <input type="submit" value="">
                            </form>
                        </div>
                        <!--/profile_details-->
                        <div class="profile_details_left">
                            <ul class="nofitications-dropdown">
                                <li class="dropdown note dra-down">
                                    <script type="text/javascript">

																	function DropDown(el) {
																		this.dd = el;
																		this.placeholder = this.dd.children('span');
																		this.opts = this.dd.find('ul.dropdown > li');
																		this.val = '';
																		this.index = -1;
																		this.initEvents();
																	}
																	DropDown.prototype = {
																		initEvents : function() {
																			var obj = this;

																			obj.dd.on('click', function(event){
																				$(this).toggleClass('active');
																				return false;
																			});

																			obj.opts.on('click',function(){
																				var opt = $(this);
																				obj.val = opt.text();
																				obj.index = opt.index();
																				obj.placeholder.text(obj.val);
																			});
																		},
																		getValue : function() {
																			return this.val;
																		},
																		getIndex : function() {
																			return this.index;
																		}
																	}

																	$(function() {

																		var dd = new DropDown( $('#dd') );

																		$(document).click(function() {
																			// all dropdowns
																			$('.wrapper-dropdown-3').removeClass('active');
																		});

																	});

                                    </script>
                                </li>
                                <div class="clearfix"></div>
                            </ul>
                        </div>
                        <div class="clearfix"></div>
                        <!--//profile_details-->
                    </div>
                    <!--//menu-right-->
                    <div class="clearfix"></div>
                </div>
                <!-- //header-ends -->
                <!--outter-wp-->
                <div class="outter-wp">
                    <!--sub-heard-part-->
                    <div class="sub-heard-part">
                        <ol class="breadcrumb m-b-0">
                            <li><a href="~/Menu/Feed">Início</a></li>
                            <li class="active">Visualização de Usuário</li>
                        </ol>
                    </div>
                    <!--//sub-heard-part-->
                    <!--/profile-->
                    <h3 class="sub-tittle pro">Perfil</h3>
                    @if (ViewBag.Logado == null)
                    {

                    }
                    @if (ViewBag.Visualizacao != null)
                    {
                        <div class="profile-widget" style="background: url(../Imagens/ImagensUsuario/@ViewBag.Visualizacao.Imagem_Capa);">
                            <img src="~/Imagens/ImagensUsuario/@ViewBag.Visualizacao.Imagem_Perfil" alt="" />
                            <h3 class="titulo-verUser">@ViewBag.Visualizacao.Nome @ViewBag.Visualizacao.Sobrenome</h3>
                        </div>
                    <!--/profile-inner-->
                        <div class="profile-section-inner">
                            <div class="col-md-6 profile-info">
                                <h3 class="inner-tittle">Informações Pessoais </h3>
                                <div class="main-grid3">
                                    <div class="p-20">
                                        <div class="about-info-p">
                                            <strong>Nome Completo</strong>
                                            <br>
                                            <p class="text-muted">@ViewBag.Visualizacao.Nome @ViewBag.Visualizacao.Sobrenome</p>
                                        </div>
                                        <div class="about-info-p">
                                            <strong>Mobile</strong>
                                            <br>
                                            <p class="text-muted">@ViewBag.Visualizacao.Telefone</p>
                                        </div>
                                        <div class="about-info-p">
                                            <strong>Youtube</strong>
                                            <br>
                                            <p class="text-muted"><a class="extraInfo" href="@ViewBag.Visualizacao.Youtube"><i class="fa fa-youtube"> Youtube</i></a></p>
                                        </div>
                                        <div class="about-info-p">
                                            <strong>Facebook</strong>
                                            <br>
                                            <p class="text-muted"><a class="extraInfo" href="@ViewBag.Visualizacao.Facebook"><i class="fa fa-facebook"> Facebook</i></a></p>
                                        </div>
                                        <div class="about-info-p">
                                            <strong>Twitch</strong>
                                            <br>
                                            <p class="text-muted"><a class="extraInfo" href="@ViewBag.Visualizacao.Twitch"><i class="fa fa-twitch"> Twitch</i></a></p>
                                        </div>
                                        <div class="about-info-p">
                                            <strong>E-mail</strong>
                                            <br>
                                            <p class="text-muted"><a class="extraInfo" href="@ViewBag.Visualizacao.Email">@ViewBag.User.Email</a></p>
                                        </div>
                                        <div class="about-info-p m-b-0">
                                            <strong>Local</strong>
                                            <br>
                                            <p class="text-muted">@ViewBag.Visualizacao.Cidade, @ViewBag.Visualizacao.Estado</p>
                                        </div>
                                    </div>
                                </div>
                                <h3 class="inner-tittle">Biografia </h3>
                                <div class="main-grid3 p-skill">
                                    <p>@ViewBag.Visualizacao.Biografia</p>
                                </div>
                                <h3 class="inner-tittle two">Instrumentos </h3>
                                <div class="main-grid3">
                                @if (ViewBag.InstrumentosVisualizacao != null)
                                {
                                    foreach (var I in ViewBag.InstrumentosVisualizacao)
                                    {
                                        <div class="barra-instrumento">
                                            <img class="icon-instrumento" src="~/Imagens/IconesInstrumentos/@(I.Icone).png" /><strong>@I.Nome</strong>
                                        </div>
                                                                        <div class="skills">
                                                                            <div class="skill1" style="width:@I.Maestria%"> </div>
                                                                        </div>
                                    }
                                }
                                else
                                    {
                                        <p>Você ainda não possui instrumentos cadastrados! Cadastre-os aqui!</p>
                                    }
                                </div>
                            </div>
                            <div class="col-md-6 profile-info two">
                                <h3 class="inner-tittle">Músicas </h3>
                                <div class="main-grid3 p-skill">
                                    <ul class="timeline">

                                        @if (ViewBag.ArquivosVisualizacao != null)
                                        {
                                            foreach (var A in ViewBag.ArquivosVisualizacao)
                                            {
                                                <li>
                                                    <div class="timeline-badge info"><i class="fa fa-headphones"></i></div>
                                                    <div class="timeline-panel">
                                                        <div class="timeline-heading">
                                                            <h4 class="nome-musica"><strong>@A.Nome</strong></h4>
                                                        </div>
                                                        <div class="timeline-body">
                                                            <p class="time">@ViewBag.Visualizacao.Nome @ViewBag.Visualizacao.Sobrenome</p>
                                                            <audio controls>
                                                                <source src="~/Arquivos/@A.Nome" type="audio/mpeg">
                                                                Seu navegador não suporta esse player.
                                                            </audio>
                                                        </div>
                                                    </div>
                                                </li>
                                            }
                                        }
                                        else
                                        {
                                            <li>Este usuário não possui arquivos!</li>
                                        }

                                    </ul>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                            <!--/map-->
                            <div class="col-md-6 profile-info two">
                                <h3 class="inner-tittle">Atividade Recente </h3>
                                <div class="main-grid3 p-skill">
                                    <ul class="timeline">
                                        <li>
                                            <div class="timeline-badge info"><i class="fa fa-comment"></i></div>
                                            <div class="timeline-panel">
                                                <div class="timeline-heading">
                                                    <h4 class="timeline-title"><a href="profile.html">@ViewBag.Visualizacao.Nome @ViewBag.Visualizacao.Sobrenome</a></h4>
                                                </div>
                                                <div class="timeline-body">
                                                    <p class="time">5 Minutes ago</p>
                                                    <p>Uploaded 2 new Photos</p>
                                                </div>
                                            </div>
                                        </li>


                                    </ul>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>

                    <!--//profile-inner-->
                    <!--//profile-->
                    }
                    else
                    {
                        Response.Redirect("/Menu/Pesquisar");
                    }
                </div>
                <!--//outer-wp-->
                <!--footer section start-->
                <footer>
                    <p>© 2017 Symphonya . Todos os direitos reservados.</p>
                </footer>
                <!--//footer section end-->
            </div>
        </div>
        <!--//content-inner-->
        <!--/sidebar-menu-->
        <div class="sidebar-menu">
            <header class="logo">
                <a href="#" class="sidebar-icon"> <span class="fa fa-bars"></span> </a> <a href="~/Menu/Feed">
                    <span id="logo"> <h1 style="font: inherit;font-size: 180%;">Symphonya</h1></span>
                    <!--<img id="logo" src="" alt="Logo"/>-->
                </a>
            </header>
            <div style="border-top:1px solid rgba(69, 74, 84, 0.7)"></div>
            <!--/down-->
            <div class="down">
                @if (ViewBag.Logado == null) { }
                else {
                    <a href="~/Menu/Perfil"><img src="~/Imagens/ImagensUsuario/@ViewBag.User.Imagem_Perfil"></a>
                    <span class="name-caret">@ViewBag.User.Nome @ViewBag.User.Sobrenome</span>
                }
                @if (ViewBag.User.Nivel == 1)
                {
                    <p><strong>Administrador</strong></p>
                }
                @if (ViewBag.User.Nivel == 2)
                {
                    <p><strong>Desenvolvedor</strong></p>
                }
                <ul>
                    <li><a class="tooltips" href="~/Acesso/Sair"><span>Log out</span><i class="lnr lnr-power-switch"></i></a></li>
                </ul>
            </div>
            <!--//down-->
            <div class="menu">
                <ul id="menu">
                    @if (ViewBag.Logado == null) { }
                    else {
                        if (ViewBag.User.Nivel != 0)
                        {
                            <li id="menu-academico">
                                <a href="#"><i class="fa fa-area-chart"></i> <span> Administração</span> <span class="fa fa-angle-right" style="padding-left: 17%;"></span></a>
                                <ul id="menu-academico-sub">
                                    <li id="menu-academico-avaliacoes"><a href="~/Administracao/Arquivos">Arquivos</a></li>
                                    <li id="menu-academico-avaliacoes"><a href="~/Administracao/Bandas">Bandas</a></li>
                                    <li id="menu-academico-boletim"><a href="~/Administracao/Usuarios">Usuários</a></li>
                                </ul>
                            </li>
                        }
                    }
                    <li id="menu-academico">
                        <a href="#"><i class="fa fa-calendar"></i> <span> Agenda & Eventos</span> <span class="fa fa-angle-right" style="padding-left: 17%;"></span></a>
                        <ul id="menu-academico-sub">
                            <li id="menu-academico-boletim"><a href="~/Menu/Agenda">Agenda</a></li>
                            <li id="menu-academico-avaliacoes"><a href="calender.html">Horários</a></li>

                        </ul>
                    </li>
                    <li><a href="~/Menu/Arquivo"><i class="fa fa-file"></i> <span>Arquivos</span></a></li>
                    <li id="menu-academico">
                        <a href="#"><i class="fa fa-group"></i> <span>Bandas</span> <span class="fa fa-angle-right" style="padding-left: 53%;"></span></a>
                        <ul id="menu-academico-sub">
                            <li id="menu-academico-avaliacoes"><a href="~/Menu/CadastroBanda">Nova Banda</a></li>
                            <li id="menu-academico-boletim"><a href="~/Menu/VerBandas">Ver Bandas</a></li>
                        </ul>
                    </li>
                    <li id="menu-academico">
                        <a href="~/Menu/VerSeguidores"><i class="fa fa-address-book"></i> <span>Seguidores</span></a>
                    </li>
                    <li>
                        <a href="~/Menu/VerSeguidos"><i class="fa fa-address-book-o"></i> <span>Seguindo</span></a>
                    </li>
                    <li>
                        <a href="~/Menu/Livestream"><i class="fa fa-twitch"></i> <span>Livestream</span></a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="clearfix"></div>
    </div>
    </div>
    <script>
							var toggle = true;

							$(".sidebar-icon").click(function() {
							  if (toggle)
							  {
								$(".page-container").addClass("sidebar-collapsed").removeClass("sidebar-collapsed-back");
								$("#menu span").css({"position":"absolute"});
							  }
							  else
							  {
								$(".page-container").removeClass("sidebar-collapsed").addClass("sidebar-collapsed-back");
								setTimeout(function() {
								  $("#menu span").css({"position":"relative"});
								}, 400);
							  }

											toggle = !toggle;
										});
    </script>
    <!--js -->
    <link rel="stylesheet" href="~/CSS/vroom.css">
    <script type="text/javascript" src="~/JS/vroom.js"></script>
    <script type="text/javascript" src="~/JS/TweenLite.min.js"></script>
    <script type="text/javascript" src="~/JS/CSSPlugin.min.js"></script>
    <script src="~/JS/jquery.nicescroll.js"></script>
    <script src="~/JS/scripts.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="~/JS/bootstrap.min.js"></script>
</body>
</html>
