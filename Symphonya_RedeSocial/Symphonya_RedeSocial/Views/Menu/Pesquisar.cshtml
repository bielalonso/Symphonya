    <link rel="icon" href="~/Imagens/symphonyaIcon.png" type="image" />
    <link rel="shortcut icon" href="~/Imagens/symphonyaIcon.png" type="image" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <meta name="keywords" content="Augment Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Bootstrap Core CSS -->
    <link href="~/CSS/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom CSS -->
    <link href="~/CSS/style.css" rel='stylesheet' type='text/css' />
    <!-- Graph CSS -->
    <link href="~/CSS/font-awesome.css" rel="stylesheet">
    <!-- jQuery -->
    <link href='//fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css'>
    <!-- lined-icons -->
    <link rel="stylesheet" href="~/CSS/icon-font.min.css" type='text/css' />
    <!-- //lined-icons -->
    <script src="~/JS/jquery-1.10.2.min.js"></script>
    <script src="~/JS/amcharts.js"></script>
    <script src="~/JS/serial.js"></script>
    <script src="~/JS/light.js"></script>
    <script src="~/JS/radar.js"></script>
    <link href="~/CSS/barChart.css" rel='stylesheet' type='text/css' />
    <link href="~/CSS/fabochart.css" rel='stylesheet' type='text/css' />
    <!--clock init-->
    <script src="~/JS/css3clock.js"></script>
    <!--Easy Pie Chart-->
    <!--skycons-icons-->
    <script src="~/JS/skycons.js"></script>

    <script src="~/JS/jquery.easydropdown.js"></script>

    <!--//skycons-icons-->
</head>
<body>
    <div class="page-container">
        <!--/content-inner-->
        <div class="left-content">
            <div class="inner-content">
                <!-- header-starts -->
                <div class="header-section">
                    <!--menu-right-->
                    <div class="top_menu">
                        <div class="main-search">
                            <form method="post" action="~/Menu/Pesquisar">
                                <input id="busca" name="busca" type="text" value="Buscar" onFocus="this.value = '';" onBlur="if (this.value == '') {this.value = 'Buscar';}" class="text" />
                                <input type="submit" value="">
                            </form>
                        </div>

                        <!--/profile_details-->
                        <div class="profile_details_left">
                            <ul class="nofitications-dropdown">
                                <li class="dropdown note dra-down">
                                    <script type="text/javascript">

																	function DropDown(el) {
																		this.dd = el;
																		this.placeholder = this.dd.children('span');
																		this.opts = this.dd.find('ul.dropdown > li');
																		this.val = '';
																		this.index = -1;
																		this.initEvents();
																	}
																	DropDown.prototype = {
																		initEvents : function() {
																			var obj = this;

																			obj.dd.on('click', function(event){
																				$(this).toggleClass('active');
																				return false;
																			});

																			obj.opts.on('click',function(){
																				var opt = $(this);
																				obj.val = opt.text();
																				obj.index = opt.index();
																				obj.placeholder.text(obj.val);
																			});
																		},
																		getValue : function() {
																			return this.val;
																		},
																		getIndex : function() {
																			return this.index;
																		}
																	}

																	$(function() {

																		var dd = new DropDown( $('#dd') );

																		$(document).click(function() {
																			// all dropdowns
																			$('.wrapper-dropdown-3').removeClass('active');
																		});

																	});

                                    </script>
                                </li>
                                <li class="dropdown note">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-diamond"></i></a>
                                    <ul class="dropdown-menu two first">
                                        @if (ViewBag.Ranking != null)
                                        {
                                            <li>
                                                <div class="notification_header">
                                                    <h3>Ranking</h3>
                                                </div>
                                            </li>
                                            foreach (var R in ViewBag.Ranking)
                                            {
                                                <li>
                                                    <a href="/Menu/MostrarUsuario/@R.ID">
                                                        <div class="user_img"><img src="~/Imagens/ImagensUsuario/@R.Imagem_Perfil" alt=""></div>
                                                        <div class="notification_desc">
                                                            <p>@R.Nome @R.Sobrenome</p>
                                                            <p><span>Músico</span></p>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                    </a>
                                                </li>
                                            }
                                        }
                                    </ul>
                                </li>

                                <li class="dropdown note">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        <i class="fa fa-universal-access"></i>
                                        @if (ViewBag.QntADM != null)
                                        {
                                            <span class="badge">@ViewBag.QntADM</span>
                                        }
                                    </a>

                                    <ul class="dropdown-menu two">
                                        @if (ViewBag.Administradores != null)
                                        {
                                            <li>
                                                <div class="notification_header">
                                                    <h3>Administradores</h3>
                                                </div>
                                            </li>
                                            foreach (var A in ViewBag.Administradores)
                                            {
                                                <li>
                                                    <a href="/Menu/MostrarUsuario/@A.ID">
                                                        <div class="user_img"><img src="~/Imagens/ImagensUsuario/@A.Imagem_Perfil" alt=""></div>
                                                        <div class="notification_desc">
                                                            <p>@A.Nome @A.Sobrenome</p>
                                                            <p><span>Administrador</span></p>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                    </a>
                                                </li>
                                            }
                                        }
                                        else
                                        {
                                            <li>
                                                <div class="notification_header">
                                                    <h3>Não Disponível</h3>
                                                </div>
                                            </li>
                                        }
                                    </ul>
                                </li>
                                <div class="clearfix"></div>
                            </ul>
                        </div>
                        <div class="clearfix"></div>
                        <!--//profile_details-->
                    </div>
                    <!--//menu-right-->
                    <!--footer section start-->
                    <footer>
                        <p>© 2017 Symphonya . Todos os direitos reservados.</p>
                    </footer>
                    <!--footer section end-->
                </div>
            </div>

            <!--!!!!!!!!!!!!!!CONTEUDO DA PAGINA-->
            <div class="mailbox-content">
                <div class="mail-toolbar clearfix">
                    <div class="float-left">
                        <div class="btn-group m-r-sm mail-hidden-options" style="display: inline-block;">
                            <div class="btn-group">
                                <a class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-tags"></i> <span class="caret"></span></a>
                                <ul class="dropdown-menu dropdown-menu-right" role="menu">
                                    <li><a href="#">Work</a></li>
                                    <li><a href="#">Family</a></li>
                                    <li><a href="#">Social</a></li>

                                    <li><a href="#">Primary</a></li>
                                    <li><a href="#">Promotions</a></li>
                                    <li><a href="#">Forums</a></li>
                                </ul>
                            </div>
                        </div>


                    </div>

                </div>
                @if (ViewBag.Usuarios != null)
                {
                <table class="table">
                    <tbody>
                        @foreach (var U in ViewBag.Usuarios)
                        {
                        <tr class="table-row">
                            <td class="table-img">
                                <img src="~/Imagens/ImagensUsuario/@(U.Imagem_Perfil)" alt="" />
                            </td>
                            <td class="table-text">
                                <h6><a class="nomeUserTabela" href="/Menu/MostrarUsuario/@U.ID">@(U.Nome) @(U.Sobrenome)</a></h6>
                                <p>@(U.Email)</p>
                            </td>
                            <td>
                                <span class="fam">Usuário</span>
                            </td>
                            <td>
                                <a class="btnSeguir" href="~/Menu/Follow/@U.ID">
                                    <img href="~/Menu/Follow/@U.ID" class="status" src="~/Imagens/iconeHover.png"
                                         onmouseover="this.src='/Imagens/icone.png'"
                                         onmouseout="this.src = '/Imagens/iconeHover.png'" />
                                    Follow
                                </a>
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
                }
                @if (ViewBag.Bandas != null)
                {
                    <table class="table">
                        <tbody>
                            @foreach (var B in ViewBag.Bandas)
                            {
                                <tr class="table-row">
                                    <td class="table-img">
                                        <img src="~/Imagens/ImagensBandas/@B.Imagem_Perfil_Banda" alt="" />
                                    </td>
                                    <td class="table-text">
                                        <h6><a href="/Menu/VerBandas/@B.ID">@(B.Nome)</a></h6>
                                        <p>@(B.Descricao.Substring(0, Math.Min(B.Descricao.Length, 100)) + "...")</p>
                                    </td>
                                    <td>
                                        <span class="fam">Banda</span>
                                    </td>
                                    <td class="march">
                                        in 5 days
                                    </td>

                                    <td>
                                        <i class="fa fa-heart"></i>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
                @if(ViewBag.Usuarios == null && ViewBag.Bandas == null)
                {
                    <h2>Não foi encontrado nenhum resultado!</h2>
                }
            </div>

            <!--//content-inner-->
            <!--/sidebar-menu-->
            <div class="sidebar-menu">
                <header class="logo">
                    <a href="#" class="sidebar-icon"> <span class="fa fa-bars"></span> </a> <a href="~/Menu/Feed">
                        <span id="logo"> <h1 style="font: inherit;font-size: 180%;">Symphonya</h1></span>
                        <!--<img id="logo" src="" alt="Logo"/>-->
                    </a>
                </header>
                <div style="border-top:1px solid rgba(69, 74, 84, 0.7)"></div>
                <!--/down-->
                <div class="down">
                    @if (ViewBag.Logado == null) { }
                    else {
                        <a href="~/Menu/Perfil"><img src="~/Imagens/ImagensUsuario/@ViewBag.User.Imagem_Perfil"></a>
                        <span class="name-caret">@ViewBag.User.Nome @ViewBag.User.Sobrenome</span>
                    }
                    @if (ViewBag.User.Nivel == 1)
                    {
                        <p><strong>Administrador</strong></p>
                    }
                    @if (ViewBag.User.Nivel == 2)
                    {
                        <p><strong>Desenvolvedor</strong></p>
                    }
                    <ul>
                        <li><a class="tooltips" href="~/Acesso/Sair"><span>Log out</span><i class="lnr lnr-power-switch"></i></a></li>
                    </ul>
                </div>
                <!--//down-->
                <div class="menu">
                    <ul id="menu">
                        @if (ViewBag.Logado == null) { }
                        else {
                            if (ViewBag.User.Nivel != 0)
                            {
                                <li id="menu-academico">
                                    <a href="#"><i class="fa fa-area-chart"></i> <span> Administração</span> <span class="fa fa-angle-right" style="padding-left: 17%;"></span></a>
                                    <ul id="menu-academico-sub">
                                        <li id="menu-academico-avaliacoes"><a href="~/Administracao/Arquivos">Arquivos</a></li>
                                        <li id="menu-academico-avaliacoes"><a href="~/Administracao/Bandas">Bandas</a></li>
                                        <li id="menu-academico-boletim"><a href="~/Administracao/Usuarios">Usuários</a></li>
                                    </ul>
                                </li>
                            }
                        }
                        <li id="menu-academico">
                            <a href="#"><i class="fa fa-calendar"></i> <span> Agenda & Eventos</span> <span class="fa fa-angle-right" style="padding-left: 17%;"></span></a>
                            <ul id="menu-academico-sub">
                                <li id="menu-academico-boletim"><a href="~/Menu/Agenda">Agenda</a></li>
                                
                            </ul>
                        </li>
                        <li><a href="~/Menu/Arquivo"><i class="fa fa-file"></i> <span>Arquivos</span></a></li>
                        <li id="menu-academico">
                            <a href="#"><i class="fa fa-group"></i> <span>Bandas</span> <span class="fa fa-angle-right" style="padding-left: 53%;"></span></a>
                            <ul id="menu-academico-sub">
                                <li id="menu-academico-avaliacoes"><a href="~/Menu/CadastroBanda">Nova Banda</a></li>
                                <li id="menu-academico-boletim"><a href="~/Menu/VerBandas">Ver Bandas</a></li>
                            </ul>
                        </li>
                        <li id="menu-academico">
                            <a href="~/Menu/VerSeguidores"><i class="fa fa-address-book"></i> <span>Seguidores</span></a>
                        </li>
                        <li>
                            <a href="~/Menu/VerSeguidos"><i class="fa fa-address-book-o"></i> <span>Seguindo</span></a>
                        </li>
                        <li>
                            <a href="~/Menu/Livestream"><i class="fa fa-twitch"></i> <span>Livestream</span></a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <script>
							var toggle = true;

							$(".sidebar-icon").click(function() {
							  if (toggle)
							  {
								$(".page-container").addClass("sidebar-collapsed").removeClass("sidebar-collapsed-back");
								$("#menu span").css({"position":"absolute"});
							  }
							  else
							  {
								$(".page-container").removeClass("sidebar-collapsed").addClass("sidebar-collapsed-back");
								setTimeout(function() {
								  $("#menu span").css({"position":"relative"});
								}, 400);
							  }

											toggle = !toggle;
										});
    </script>
    <!--js -->
    <link rel="stylesheet" href="~/CSS/vroom.css">
    <script type="text/javascript" src="~/JS/vroom.js"></script>
    <script type="text/javascript" src="~/JS/TweenLite.min.js"></script>
    <script type="text/javascript" src="~/JS/CSSPlugin.min.js"></script>
    <script src="~/JS/jquery.nicescroll.js"></script>
    <script src="~/JS/scripts.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="~/JS/bootstrap.min.js"></script>
</body>
